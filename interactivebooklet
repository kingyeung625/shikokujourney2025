    // --- Helper Functions ---
    let currentDayIdx = 0;
    function getBooking(day, idx) { return localStorage.getItem(`book_v28_${day}_${idx}`); }
    function setBooking(day, idx, url) { if(url) localStorage.setItem(`book_v28_${day}_${idx}`, url); else localStorage.removeItem(`book_v28_${day}_${idx}`); renderTimeline(); }

    function init() {
        renderNav();
        renderTimeline();
        renderHotels();
    }

    function renderNav() {
        const nav = document.getElementById('nav-tabs');
        nav.innerHTML = '';
        itinerary.forEach((day, idx) => {
            const btn = document.createElement('button');
            btn.className = `date-btn ${idx === currentDayIdx ? 'active' : ''}`;
            btn.textContent = day.date;
            btn.onclick = () => {
                currentDayIdx = idx;
                renderNav();
                renderTimeline();
            };
            nav.appendChild(btn);
        });
    }

    function calculateDuration(start, end) {
        if (!start || !end || start === '-' || end === '') return '';
        const [h1, m1] = start.split(':').map(Number);
        const [h2, m2] = end.split(':').map(Number);
        if (isNaN(h1) || isNaN(h2)) return '';
        let diff = (h2 * 60 + m2) - (h1 * 60 + m1);
        if (diff < 0) diff += 24 * 60; 
        const hours = Math.floor(diff / 60);
        const mins = diff % 60;
        if (hours > 0 && mins > 0) return `${hours}h ${mins}m`;
        if (hours > 0) return `${hours}h`;
        return `${mins}m`;
    }

    function renderTimeline() {
        const container = document.getElementById('timeline');
        container.innerHTML = '';
        const day = itinerary[currentDayIdx];
        
        day.events.forEach((item, idx) => {
            const mapLink = `https://www.google.com/maps/search/?api=1&query=${encodeURIComponent(item.title)}`;
            const duration = calculateDuration(item.time, item.end);
            const savedUrl = getBooking(currentDayIdx, idx);
            
            let mustHtml = '';
            if(item.mustDo) mustHtml += `<div class="must-box do"><span class="must-label">âœ¨ ä¸€å®šè¦å®Œæˆ</span><ul class="must-list">${item.mustDo.map(x=>`<li>${x}</li>`).join('')}</ul></div>`;
            if(item.mustKnow) mustHtml += `<div class="must-box know"><span class="must-label">ğŸ§  ä¸€å®šè¦çŸ¥é“</span><ul class="must-list">${item.mustKnow.map(x=>`<li>${x}</li>`).join('')}</ul></div>`;
            if(item.mustEat) mustHtml += `<div class="must-box eat"><span class="must-label">ğŸ½ï¸ ä¸€å®šè¦åƒ</span><ul class="must-list">${item.mustEat.map(x=>`<li>${x}</li>`).join('')}</ul></div>`;

            // Booking Button Logic
            let bookBtn = '';
            if(savedUrl) {
                bookBtn = `
                    <button class="btn-base book-btn booked" onclick="window.open('${savedUrl}', '_blank')">âœ… å·²é ç´„ (æŸ¥çœ‹)</button>
                    <button class="btn-base edit-book-btn" onclick="editBooking(${currentDayIdx}, ${idx})">âœï¸</button>
                `;
            } else {
                bookBtn = `<button class="btn-base book-btn" onclick="addBooking(${currentDayIdx}, ${idx})">â¬œ é»æ­¤è¨˜éŒ„é ç´„</button>`;
            }

            // Mapcode Button Logic
            let mapBtn = '';
            if(item.mapcode && item.mapcode !== '-') {
                 mapBtn = `<div class="mapcode-container" onclick="showBigMapcode('${item.mapcode}')"><span class="mapcode-label">MAPCODE</span><span class="mapcode-val">${item.mapcode}</span></div>`;
            }
            
            // External Link Buttons
            let linkBtns = '';
            if(item.tabelog) linkBtns += `<a href="${item.tabelog}" target="_blank" class="btn-base tabelog-btn">ğŸŸ  Tabelog</a>`;
            if(item.url) linkBtns += `<a href="${item.url}" target="_blank" class="btn-base official-btn">ğŸŒ å®˜ç¶²</a>`;
            if(item.tripadvisor) linkBtns += `<a href="${item.tripadvisor}" target="_blank" class="btn-base trip-btn">ğŸ¦‰ TripAdvisor</a>`;

            // Parking Tip
            let parkingHtml = '';
            if (item.desc.includes('åœè»Š')) {
                parkingHtml = `<div class="parking-info">ğŸ…¿ï¸ ç•™æ„åœè»Šå ´ä½ç½®</div>`;
            }
            
            // Drive time badge
            let driveBadge = item.drive && item.drive !== '-' ? `<div class="drive-badge">ğŸš— ${item.drive}</div>` : '';
            
            // Header Image Logic
            let headerStyle = item.gradient;
            let headerClass = `visual-header ${item.gradient}`;
            if (item.img) {
                headerStyle = `background-image: url('${item.img}')`;
                headerClass += ' has-img';
            }

            const html = `
                <div class="timeline-row">
                    <div class="time-col">
                        <div class="start-time">${item.time}</div>
                        ${duration ? `<div class="duration-badge">${duration}</div>` : ''}
                        <div class="end-time">${item.end || ''}</div>
                    </div>
                    
                    <div class="line-col">
                        <div class="dot"></div>
                        ${driveBadge}
                    </div>

                    <div class="content-col">
                        <div class="card">
                            <div class="${headerClass}" style="${item.img ? headerStyle : ''}">
                                <div class="visual-icon">${item.icon}</div>
                                <a href="${mapLink}" target="_blank" class="map-float-btn">ğŸ“ å°èˆª</a>
                            </div>

                            <div class="card-body">
                                <h3 class="card-title">${item.title}</h3>
                                
                                <div class="long-desc">${item.desc}</div>

                                ${mustHtml}
                                ${parkingHtml}

                                <div class="card-footer">
                                    <div class="info-group">
                                        <span class="info-tag">ğŸ’° ${item.price}</span>
                                        <span class="info-tag">ğŸ•’ ${item.open}</span>
                                    </div>
                                    
                                    <div class="action-group">
                                        ${linkBtns}
                                        ${bookBtn}
                                        ${mapBtn}
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            `;
            container.insertAdjacentHTML('beforeend', html);
        });
    }

    function addBooking(day, idx) {
        const url = prompt("è«‹è²¼ä¸Šæ‚¨çš„é ç´„ç¢ºèªé€£çµ (Google Drive æˆ– PDF ç¶²å€):");
        if(url) setBooking(day, idx, url);
    }

    function editBooking(day, idx) {
        if(confirm("è¦ç§»é™¤ç›®å‰çš„é ç´„é€£çµå—ï¼Ÿ")) setBooking(day, idx, null);
    }

    function showBigMapcode(code) {
        document.getElementById('bigMapcodeText').innerText = code;
        openModal('mapcodeModal');
    }

    function searchNearby(type) {
        let query = "";
        switch(type) {
            case 'gas_station': query = "gas station"; break;
            case 'parking': query = "parking"; break;
            case 'convenience_store': query = "convenience store"; break;
        }
        if(query) window.open(`https://www.google.com/maps/search/${query}/`, '_blank');
    }

    function renderHotels() {
        const list = document.getElementById('hotel-list');
        hotels.forEach(h => {
            list.innerHTML += `
                <div class="hotel-row">
                    <span class="hotel-name">${h.name}</span>
                    <div style="font-size:0.85rem; color:#666; margin-bottom:5px;">${h.note}</div>
                    <div style="display:flex; justify-content:space-between; align-items:center;">
                        <span style="font-family:monospace; font-size:1.1rem; color:var(--primary);">${h.tel}</span>
                        <button class="copy-btn" onclick="copyText('${h.tel}')">è¤‡è£½</button>
                    </div>
                </div>
            `;
        });
    }

    function toggleModal(show) { document.getElementById('hotelModal').style.display = show ? 'flex' : 'none'; }
    function openModal(id) { document.getElementById(id).style.display = 'flex'; }
    function closeModal(id) { document.getElementById(id).style.display = 'none'; }
    function copyText(text) { navigator.clipboard.writeText(text).then(()=>alert('å·²è¤‡è£½ï¼š'+text)); }

    init();
</script>

</body>
</html>
